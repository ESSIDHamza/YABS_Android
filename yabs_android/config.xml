<?xml version='1.0' encoding='utf-8'?>
<widget id="tn.rnu.insat.essid.hamza" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>YABS</name>
    <description>
        Application mobile de la librairie en ligne Yet Another BookStore.
    </description>
    <author email="hamza931222@gmail.com" href="http://hamza-essid.fr">
        Hamza ESSID
    </author>
    <content src="template/index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
